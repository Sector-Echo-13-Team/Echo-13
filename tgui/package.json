{
  "private": true,
  "name": "tgui-workspace",
  "version": "4.3.0",
  "packageManager": "yarn@3.4.1",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "tgui:analyze": "webpack --analyze",
    "tgui:bench": "webpack --env TGUI_BENCH=1 && node packages/tgui-bench/index.js",
    "tgui:build": "webpack",
    "tgui:dev": "node --experimental-modules packages/tgui-dev-server/index.js",
    "tgui:lint": "eslint packages --ext .js,.cjs,.ts,.tsx",
    "tgui:prettier": "prettier --check .",
    "tgui:sonar": "eslint packages --ext .js,.cjs,.ts,.tsx -c .eslintrc-sonar.yml",
    "tgui:test": "jest --watch",
    "tgui:test-simple": "CI=true jest --color",
    "tgui:test-ci": "CI=true jest --color --collect-coverage",
    "tgui:tsc": "tsc"
  },
  "dependencies": {
    "@babel/core": "^7.21.0",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-jscript": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@types/jest": "^27.5.2",
    "@types/jsdom": "^16.2.15",
    "@types/node": "^14.18.37",
    "@types/webpack": "^5.28.0",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.3.0",
    "babel-plugin-inferno": "^6.6.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "common": "workspace:*",
    "css-loader": "^5.2.7",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-radar": "^0.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-unused-imports": "^1.1.5",
    "file-loader": "^6.2.0",
    "inferno": "^7.4.11",
    "jest": "^27.5.1",
    "jest-circus": "^27.5.1",
    "jsdom": "^16.7.0",
    "mini-css-extract-plugin": "^1.6.2",
    "prettier": "^2.8.4",
    "sass": "^1.58.3",
    "sass-loader": "^11.1.1",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.3.7",
    "typescript": "^4.9.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.76.0",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^4.10.0"
  }
}
